{
  "name": "tsmean",
  "version": "1.0.0",
  "description": "Tutorial Page how to setup the Typescript-Mean project from scratch",
  "main": "index.js",
  "scripts": {
    "start": "cd dist && python -m SimpleHTTPServer 8002",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-watch",
    "compilepartials": "$(npm bin)/html-partials-compiler ./src/index.html > ./dist/index.html",
    "compile": "mkdir -p dist && cp -r img dist/img && npm run compilepartials"
  },
  "watch": {
    "compilepartials": {
      "patterns": [
        "src"
      ],
      "extensions": "*",
      "quiet": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bersling/ts-mean.git"
  },
  "keywords": [
    "Typescript",
    "MEAN"
  ],
  "author": "Daniel Niederberger <bersling@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bersling/ts-mean/issues"
  },
  "homepage": "https://github.com/bersling/ts-mean#readme",
  "devDependencies": {
    "html-partials-compiler": "^1.0.7",
    "mustache": "^2.3.0",
    "npm-watch": "^0.1.9"
  }
}
